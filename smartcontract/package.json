{
  "name": "smartcontract",
  "version": "1.0.0",
  "description": "YieldMind DeFi Protocol Smart Contracts",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:unit": "hardhat test test/*.test.ts",
    "test:integration": "hardhat test test/Integration.test.ts",
    "test:coverage": "hardhat coverage",
    "test:somnia": "hardhat test --network somniaTestnet",
    "deploy:testnet": "hardhat run scripts/deploy.ts --network somniaTestnet",
    "deploy:mainnet": "hardhat run scripts/deploy.ts --network somnia",
    "deploy:resume": "hardhat run scripts/deploy-resume.ts --network somniaTestnet",
    "deploy:final": "hardhat run scripts/deploy-final.ts --network somniaTestnet",
    "deploy:vault": "hardhat run scripts/deploy-yieldmind-vault.ts --network somniaTestnet",
    "deploy:feemanager": "hardhat run scripts/deploy-feemanager.ts --network somniaTestnet",
    "setup:permissions": "hardhat run scripts/setup-permissions.ts --network somniaTestnet",
    "setup:tokens": "hardhat run scripts/setup-somnia-tokens.ts --network somniaTestnet",
    "setup:protocols": "hardhat run scripts/setup-somnia-protocols.ts --network somniaTestnet",
    "verify:testnet": "hardhat run scripts/verify.ts --network somniaTestnet",
    "verify:mainnet": "hardhat run scripts/verify.ts --network somnia",
    "deploy:crossfi-testnet": "hardhat run scripts/deploy.ts --network crossfiTestnet",
    "deploy:crossfi-mainnet": "hardhat run scripts/deploy.ts --network crossfi",
    "node": "hardhat node",
    "clean": "hardhat clean",
    "coverage": "hardhat coverage"
  },
  "keywords": ["defi", "yield", "staking", "crossfi", "ethereum"],
  "author": "YieldMind Team",
  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "hardhat": "^2.26.1"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "dotenv": "^17.2.1"
  }
}
